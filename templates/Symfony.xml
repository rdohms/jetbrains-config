<templateSet group="Symfony">
  <template name="use_assert" value="use Symfony\Component\Validator\Constraints as Assert;" description="Imports Valdiation constraints" toReformat="true" toShortenFQNames="true">
    <context>
      <option name="JSP" value="false" />
      <option name="PHP" value="true" />
    </context>
  </template>
  <template name="use_filter" value="use DMS\Filter\Rules as Filter;" description="Import Filter Rules (DMS)" toReformat="true" toShortenFQNames="true">
    <context>
      <option name="JSP" value="false" />
      <option name="PHP" value="true" />
    </context>
  </template>
  <template name="use_orm" value="use Doctrine\ORM\Mapping as ORM;" description="ORM Import" toReformat="true" toShortenFQNames="true">
    <context>
      <option name="JSP" value="false" />
      <option name="PHP" value="true" />
    </context>
  </template>
  <template name="xlf-entry" value="&lt;trans-unit id=&quot;$entry$&quot; resname=&quot;$entry$&quot;&gt;&#10;    &lt;source&gt;$entry$&lt;/source&gt;&#10;    &lt;target&gt;__$entry$&lt;/target&gt;&#10;&lt;/trans-unit&gt;" description="Entry in Translation file" toReformat="true" toShortenFQNames="true">
    <variable name="entry" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="XML" value="true" />
    </context>
  </template>
  <template name="use_symfony_controller" value="use Sensio\Bundle\FrameworkExtraBundle\Configuration\Route;&#10;use Sensio\Bundle\FrameworkExtraBundle\Configuration\Template;&#10;use Sensio\Bundle\FrameworkExtraBundle\Configuration\ParamConverter;" description="Imports Symfony Controller Annotations" toReformat="true" toShortenFQNames="true">
    <context>
      <option name="PHP" value="true" />
    </context>
  </template>
  <template name="use_serializer" value="use JMS\Serializer\Annotation as Serializer;" description="Import Serializer Annotations" toReformat="true" toShortenFQNames="true">
    <context>
      <option name="PHP" value="true" />
    </context>
  </template>
  <template name="use_rest" value="use FOS\RestBundle\Controller\Annotations as Rest;" description="Import FOSRestBundle annotations" toReformat="true" toShortenFQNames="true">
    <context>
      <option name="PHP" value="true" />
    </context>
  </template>
</templateSet>